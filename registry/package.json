{
    "name": "registry",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "tsc": "tsc",
        "compile": "tsc --incremental",
        "dev": "rimraf build && nodemon -e ts,json5 --exec \"tsc --incremental && npm run start | pino-pretty\" ",
        "build": "npm run compile && cd ./client && npm run build",
        "start": "dotenv -- node -r source-map-support/register ./build/server/index.js",
        "start-docker": "npm run migrate && npm start",
        "knex": "knex",
        "migrate": "dotenv -- knex --knexfile config/knexfile.ts migrate:latest",
        "migrate:make": "knex --knexfile config/knexfile.ts migrate:make",
        "migrate:rollback": "dotenv -- knex --knexfile config/knexfile.ts migrate:rollback",
        "seed": "dotenv -- knex --knexfile config/knexfile.ts seed:run",
        "seed:make": "knex --knexfile config/knexfile.ts seed:make -x ts",
        "test": "cross-env-shell DB_CLIENT=sqlite3 npm run reset && cross-env NODE_ENV=test NODE_CONFIG_ENV=sqlite-test mocha",
        "test:mysql": "cross-env-shell NODE_ENV=test NODE_CONFIG_ENV=mysql-test DB_CLIENT=mysql \"npm run reset && mocha\" ",
        "test:postgres": "cross-env-shell NODE_ENV=test NODE_CONFIG_ENV=postgres-test DB_CLIENT=pg \"npm run reset && mocha\" ",
        "test:ci": "npm run migrate && cross-env NODE_ENV=test dotenv -- nyc mocha --forbid-only",
        "reset": "dotenv bash ./scripts/reset.sh && npm run migrate",
        "assetsdiscovery": "cross-env NODE_ENV=production INTERVAL=2000 node -r source-map-support/register ./build/server/runnerAssetsDiscovery.js",
        "assetsdiscovery:dev": "nodemon -e ts,json5 --exec \"npx tsc --incremental && dotenv -- npm run assetsdiscovery | pino-pretty\" "
    },
    "author": "",
    "license": "Apache-2.0",
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/bcrypt": "^5.0.2",
        "@types/chai": "^5.0.1",
        "@types/chai-as-promised": "^8.0.1",
        "@types/config": "^3.3.5",
        "@types/deep-equal": "^1.0.4",
        "@types/escape-html": "^1.0.4",
        "@types/express": "^4.17.21",
        "@types/express-serve-static-core": "^4.19.5",
        "@types/express-session": "^1.18.0",
        "@types/is-valid-domain": "0.0.4",
        "@types/jsonwebtoken": "^9.0.7",
        "@types/lodash": "^4.17.9",
        "@types/mocha": "^10.0.8",
        "@types/newrelic": "^9.14.4",
        "@types/node": "^20.16.7",
        "@types/passport": "^1.0.16",
        "@types/passport-http-bearer": "^1.0.41",
        "@types/passport-local": "^1.0.38",
        "@types/sinon": "^17.0.3",
        "@types/supertest": "^6.0.2",
        "@types/url-join": "^4.0.3",
        "@types/uuid": "^9.0.8",
        "chai": "4.5.0",
        "chai-as-promised": "^7.1.2",
        "cross-env": "7.0.3",
        "jsonwebtoken": "^9.0.2",
        "mocha": "^10.7.3",
        "nock": "^13.5.5",
        "nodemon": "^3.1.7",
