        "connect-session-knex": "^5.0.0",
        "deep-equal": "^2.2.3",
        "dotenv-cli": "^7.4.2",
        "escape-html": "^1.0.3",
        "express": "^4.21.0",
        "express-session": "^1.18.0",
        "http-shutdown": "^1.2.2",
        "ilc-plugins-sdk": "^2.1.0",
        "is-valid-domain": "0.1.6",
