{
    "name": "registry",
    "version": "1.0.0",
    "description": "",
    "main": "build/index.js",
    "scripts": {
        "tsc": "tsc",
        "compile": "tsc --incremental",
        "dev": "nodemon -e ts,json5 --exec \"npx tsc --incremental && dotenv -- npm run start\" ",
        "build": "npm run compile && cd ./client && npm run build",
        "start": "dotenv -- node -r source-map-support/register ./build/server/index.js",
        "start-docker": "npm run migrate && npm start",
        "knex": "knex",
        "migrate": "dotenv -- knex --knexfile config/knexfile.ts migrate:latest",
        "migrate:make": "knex --knexfile config/knexfile.ts migrate:make",
        "migrate:rollback": "knex --knexfile config/knexfile.ts migrate:rollback",
        "seed": "dotenv -- knex --knexfile config/knexfile.ts seed:run",
        "seed:make": "knex --knexfile config/knexfile.ts seed:make -x ts",
        "test": "cross-env NODE_ENV=test NODE_CONFIG_ENV=sqlite-test mocha",
        "test:mysql": "cross-env-shell NODE_ENV=test NODE_CONFIG_ENV=mysql-test DB_CLIENT=mysql \"npm run reset && mocha ${MOCHA_ARGS}\" ",
        "test:postgres": "cross-env-shell NODE_ENV=test NODE_CONFIG_ENV=postgres-test DB_CLIENT=pg \"npm run reset && mocha ${MOCHA_ARGS}\" ",
        "test:ci": "npm run migrate && cross-env NODE_ENV=test nyc mocha --forbid-only",
        "test:coverage": "cross-env NODE_ENV=test nyc mocha",
        "reset": "dotenv bash ./scripts/reset.sh && npm run migrate",
        "assetsdiscovery": "cross-env NODE_ENV=production INTERVAL=2000 node -r source-map-support/register ./build/server/runnerAssetsDiscovery.js",
        "assetsdiscovery:dev": "nodemon -e ts,json5 --exec \"npx tsc --incremental && dotenv -- npm run assetsdiscovery | pino-pretty\" "
    },
    "author": "",
    "license": "Apache-2.0",
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/bcrypt": "^5.0.0",
        "@types/chai": "4.3.6",
        "@types/config": "3.3.1",
        "@types/deep-equal": "^1.0.2",
        "@types/escape-html": "^1.0.2",
        "@types/express": "4.17.18",
        "@types/express-serve-static-core": "4.17.37",
        "@types/express-session": "^1.17.8",
        "@types/is-valid-domain": "0.0.2",
        "@types/lodash": "^4.14.199",
        "@types/mocha": "^10.0.2",
        "@types/newrelic": "^9.14.1",
        "@types/node": "^20.8.3",
        "@types/passport": "^1.0.13",
        "@types/passport-http-bearer": "^1.0.38",
        "@types/passport-local": "^1.0.36",
        "@types/sinon": "^10.0.19",
        "@types/supertest": "^2.0.14",
        "@types/url-join": "^4.0.1",
        "@types/uuid": "^9.0.5",
        "chai": "4.3.10",
        "cross-env": "7.0.3",
        "mocha": "^10.2.0",
        "nock": "^13.3.3",
        "nodemon": "^3.0.1",
        "nyc": "^15.1.0",
        "pino-pretty": "^10.2.3",
        "rimraf": "^5.0.5",
        "sinon": "^16.1.0",
        "supertest": "6.3.3",
        "timekeeper": "^2.3.1",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "@namecheap/error-extender": "^1.2.0",
        "@newrelic/native-metrics": "^10.0.1",
        "axios": "^0.26.1",
        "bcrypt": "^5.1.1",
        "body-parser": "^1.20.2",
        "config": "^3.3.9",
        "connect-session-knex": "^3.0.1",
        "deep-equal": "^2.2.2",
        "dotenv-cli": "^7.3.0",
        "escape-html": "^1.0.3",
        "express": "^4.18.2",
        "express-session": "^1.17.3",
        "http-shutdown": "^1.2.2",
        "ilc-plugins-sdk": "^2.1.0",
        "is-valid-domain": "0.1.6",
        "joi": "^17.11.0",
        "knex": "^3.0.1",
        "lodash": "^4.17.21",
        "mysql": "^2.18.1",
        "newrelic": "^11.1.0",
        "node-html-parser": "^6.1.10",
        "openid-client": "^5.6.0",
        "passport": "^0.6.0",
        "passport-http-bearer": "^1.0.1",
        "passport-local": "^1.0.0",
        "pg": "^8.11.3",
        "query-string": "^7.1.3",
        "serve-static": "^1.15.0",
        "source-map-support": "^0.5.21",
        "sqlite3": "^5.1.6",
        "url-join": "^4.0.1",
        "uuid": "^9.0.1"
    },
    "engines": {
        "node": "^20.8.0"
    },
    "engineStrict": true
}
